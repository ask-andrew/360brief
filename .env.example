# Auth0 Configuration
AUTH0_DOMAIN=your-tenant.region.auth0.com
AUTH0_CLIENT_ID=your-client-id
AUTH0_CLIENT_SECRET=your-client-secret
AUTH0_ISSUER_BASE_URL=https://your-tenant.region.auth0.com
AUTH0_API_AUDIENCE=https://your-tenant.region.auth0.com/api/v2/
AUTH0_BASE_URL=http://localhost:3001
AUTH0_SECRET=your-secure-random-string

# Next.js Configuration
NEXT_PUBLIC_AUTH0_BASE_URL=http://localhost:3001
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXTAUTH_SECRET=generate-a-secure-secret-here

# Application URLs
APP_BASE_URL=http://localhost:3001
APP_DASHBOARD_URL=http://localhost:3001/dashboard

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3001/api/auth/callback/google

# Google Cloud Configuration (for local development)
# Uncomment and set the path to your service account key file
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json

# Supabase Configuration
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Google Cloud Project (example values)
GCP_PROJECT_ID=your-gcp-project-id
GCP_SA_CLIENT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
# Note: Never commit actual private keys to version control
